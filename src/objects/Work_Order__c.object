    <validationRules>
        <fullName>Beta_Due_Date_greater_than_Due_Date</fullName>
        <active>true</active>
        <errorConditionFormula>Beta_Due_Date__c  &gt; Due_Date__c</errorConditionFormula>
        <errorDisplayField>Beta_Due_Date__c</errorDisplayField>
        <errorMessage>Beta Due Date cannot be greater than Due Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Must_have_Due_Dates</fullName>
        <active>true</active>
        <errorConditionFormula>AND( OR(ISPICKVAL( Stage__c , &quot;Development&quot;),ISPICKVAL(Stage__c,&quot;Beta&quot;))   ,  OR(ISNULL( Beta_Due_Date__c ), ISNULL( Due_Date__c ) )  )</errorConditionFormula>
        <errorMessage>You must enter a Beta Due Date and a Due Date for the Work Order.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Project_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(Stage__c ,&quot;Client Approved&quot;),  ISBLANK(Project__c  ))</errorConditionFormula>
        <errorDisplayField>Project__c</errorDisplayField>
        <errorMessage>Please enter a Project.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Tech_Lead</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(Stage__c ,&quot;Client Approved&quot;),  ISBLANK(Technical_Lead__c ))</errorConditionFormula>
        <errorDisplayField>Technical_Lead__c</errorDisplayField>
        <errorMessage>Please enter a Technical Lead. Use Kalyan as a default if you are not sure who will be working on your work order.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Withdrawn_Reason</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISPICKVAL( Stage__c ,&quot;Withdrawn&quot;) , ISBLANK( Withdrawn_Reason__c ) )</errorConditionFormula>
        <errorDisplayField>Withdrawn_Reason__c</errorDisplayField>
        <errorMessage>Please enter the reason with withdraw of this work order.</errorMessage>
    </validationRules>
