<apex:page standardController="Appirio_PSAe__Proj__c" recordSetVar="projects" sidebar="false">

    <apex:pageBlock >
        <apex:form >
            <apex:selectList value="{!filterid}" size="1">
                <apex:selectOptions value="{!listviewoptions}"/>
            </apex:selectList>
            <apex:commandButton value="Go" action="{!list}"/>
        </apex:form>
        <apex:pageBlockButtons >
            <apex:form >
                <apex:commandButton value="Save" action="{!save}"/>
            </apex:form>
        </apex:pageBlockButtons>
        <apex:form >
            <apex:pageBlockTable value="{!projects}" var="proj">
                <apex:column headerValue="Project Type" value="{!proj.RecordTypeId}"/>
                <apex:column headerValue="Project" value="{!proj.name}"/>
                <apex:column headerValue="Start Date" value="{!proj.Appirio_PSAe__Start_Date__c}"/>
                <apex:column headerValue="Trial Migration" value="{!proj.Trial_Migration_Complete__c}"/>
                <apex:column headerValue="Initial Walk Through" value="{!proj.Initial_Walk_Through_Date__c}"/>
                <apex:column headerValue="Designer QA" value="{!proj.Designer_QA_Complete__c}"/>
                <apex:column headerValue="User Testing/Final Walk Through" value="{!proj.User_Testing_Complete__c}"/>
                <apex:column headerValue="End User Training" value="{!proj.End_User_Training__c}"/>
                <apex:column headerValue="Final Migration Complete" value="{!proj.Final_Migration_Complete_Go_Live__c}"/>
                <apex:column headerValue="End Date" value="{!proj.Appirio_PSAe__End_Date__c}"/>
                <apex:inlineEditSupport event="ondblClick"/>
            </apex:pageBlockTable>
        </apex:form>
    </apex:pageBlock>
  <!-- Begin Default Content REMOVE THIS -->
  <h1>Congratulations</h1>
  This is your new Page: BRScheduleUpdate
  <!-- End Default Content REMOVE THIS -->
</apex:page>